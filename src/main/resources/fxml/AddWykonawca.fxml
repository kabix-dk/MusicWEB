<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pb.wi.musicweb.controllers.WykonawcaController">
   <children>
      <HBox alignment="CENTER" spacing="20.0">
         <children>
            <Label text="%name.wykonawca" />
            <TextField fx:id="nameTextField" />
            <Label text="%surname.wykonawca" />
            <TextField fx:id="surnameTextField" />
            <Label text="%nick.wykonawca" />
            <TextField fx:id="nickTextField" />
            <Button fx:id="addButton" mnemonicParsing="false" onAction="#addWykonawcaOnAction" text="%add.button" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
